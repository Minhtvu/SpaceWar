accelerationY = 0;
if (lKey)
{
   image_angle += 3;
}

if (rKey)
{
   image_angle -= 3;;
}
//Calculate velocity
//Move the ship
x  += velocityX;
y  += velocityY;
//Keep not out of screen
if ( y &lt;= 0){
    y = room_height -1}
else if (y &gt;= room_height){
    y = 1;}
if ( x &lt;= 0){
    x = room_width -1}
else if (x &gt;= room_width){
    x = 1;}
//Shooting
if (uKey &amp;&amp; cooldown = false)
{
    instance_create(x,y, obj_Missile2);
    cooldown = true;
    alarm[0] = 5;}
</string>
