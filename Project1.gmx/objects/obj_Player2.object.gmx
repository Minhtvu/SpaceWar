if (dKey)
{
   accelerationX = 0.1*cos(degtorad(image_angle));
   accelerationY = -0.1*sin(degtorad(image_angle));
   oldX = velocityX;
   oldY = velocityY;
   velocityX += accelerationX;
   velocityY += accelerationY;
